<?xml version="1.0" ?>
<openerp>
	<data>

        <record id="edit_project_document" model="ir.ui.view">
            <field name="name">project.project.form.document</field>
            <field name="model">project.project</field>
            <field name="inherit_id" ref="project.edit_project"/>
            <field name="arch" type="xml">
                <page string="Project Stages" position="before" >
                    <page string="Document pages" >
                        <field name="page_ids" nolabel="1"/>
                    </page>
                </page>
            </field>
    	</record>
        <record id="view_task_form_wiki" model="ir.ui.view">
            <field name="name">project.task.form.wiki</field>
            <field name="model">project.task</field>
            <field name="inherit_id" ref="project.view_task_form2"/>
            <field name="arch" type="xml">
                <page string="Extra Info" position="after" >
                    <page string="Document pages" >
                        <field name="page_ids" nolabel="1"/>
                    </page>
                </page>
            </field>
    	</record>
	</data>
</openerp>
